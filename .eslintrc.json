{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "off",
        "semi": "off",
        "eol-last": "off",
        "indent": "off",
        "no-use-before-define": "off"
    }
}
